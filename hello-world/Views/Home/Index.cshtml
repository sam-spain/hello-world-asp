@{
}
<p>Welcome to my page!</p>
@foreach (var book in Model)
{


<div>
    <h2>@book.Title</h2>
</div>
<div>
    <h2>
        @book.Author
    </h2></div>
<div>
    @book.PublishDate
</div>
<div>
    <strong>@book.Price</strong>
</div>
<div>
    @book.Description
</div>
}